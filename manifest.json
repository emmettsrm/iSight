{
    "manifest_version": 2,
  
    "name": "iSight Graph Interpreter",
    "description": "This extension interprets graphical data for those requiring assistance interpreting visual data.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": [
      "activeTab",
      "https://ajax.googleapis.com/",
      "storage",
      "declarativeContent",
      "activeTab"
    ]
  }